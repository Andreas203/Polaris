  <body data-spy="scroll" data-target="#navbar1" data-offset="60">
    <header class="bg-primary">
        <div class="container h-100">
            <div class="row h-100">
                <div class="col-12">
                    <div class="text-center m-0 vh-100 d-flex flex-column justify-content-center text-light">
                        <h1 class="display-4">Polaris Analytics</h1>
                        <p class="lead">Polaris-Lickin' Good.</p>
                        <div class="row">
                            <div class="col-lg-4 col-sm-7 mx-auto">
                              <%= form_tag(welcome_index_path, :method => "get", class: 'input-group mb-3') do %>
                                <%= search_field_tag :search, params[:search], placeholder: "Search...", class: "form-control form-control-lg", style:"border-top-left-radius:0.3rem; border-bottom-left-radius:0.3rem;"%>
                                <div class="input-group-append">
                                  <%= submit_tag("Search", :class=>"btn btn-outline-light btn-lg rounded-right") %>
                                </div>

                              <% end %>
                                </div>
                            </div>

                        </div>
                    </div>
                    <div class="container" style="margin-top: -160px !important; margin-bottom: 50px;">
                    <% @results.each do |result| %>
                    <div class="col-lg-12 mb-4">
                      <div class="card border-primary h-100 copyable" style="align-items: center !important;">
                          <div class="card-body d-flex flex-column align-items-start" style="align-items: center !important;">
                              <h4 class="card-title text-primary"><% result %></h4>
                              <p class="card-text"><% result %></p>
                              <a href="#" class="btn btn-primary mt-auto copyable">View</a>
                          </div>
                      </div>
                  </div>
                  <% end %>
                </div>



                  </div>
                  </div>


    </header>

